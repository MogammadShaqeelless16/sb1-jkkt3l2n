{
  "expo": {
    "name": "Uthutho",
    "slug": "uthutho",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "uthutho",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d57defc0-07eb-42d8-9c0c-e0c8df94ce4a"
      }
    },
    "android": {
      "package": "com.shaqeel16.uthutho"
    },
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "backgroundColor": "#000000"
    }
  }
}
